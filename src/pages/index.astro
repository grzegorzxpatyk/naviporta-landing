---
import Layout from '../layouts/Layout.astro'
import Hero from '../components/blocks/hero/HomeCTA.astro'
import Features from '../components/blocks/features/FeatureCards.astro'
import Testimonial from '../components/blocks/testimonials/BasicDark.astro'

import testimonialBackground from '../assets/satisfiedCustomer.jpg'
import Maps from '../components/blocks/maps/Maps.astro'
// - SEO
const SEO = {
	title: 'Tachografy Naviporta',
	description:
		'Naviporta - Tachografy. Serwis tachografów Miechów, Wolbrom, Olkusz, Kraków. Firma zajmuje się kalibracją, naprawą Tachografów Cyfrowych marek VDO i Stoneridge oraz wymianą Tachografów cyfrowych firmy VDO.'
}
---

<Layout title={SEO.title} description={SEO.description}>
	<Hero />
	<Features />
	<Testimonial bg={testimonialBackground} bgPosition="right" />
	<Maps />
</Layout>
